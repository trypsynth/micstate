@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
include(SelectLibraryConfigurations)
include(FindPackageHandleStandardArgs)

if(NOT TARGET universalspeech::universalspeech)
	add_library(universalspeech::universalspeech STATIC IMPORTED)
	find_library(UNIVERSALSPEECH_LIBRARY_RELEASE
		NAMES UniversalSpeechStatic
		PATHS "${CMAKE_CURRENT_LIST_DIR}/../../lib"
		NO_DEFAULT_PATH
	)
	find_library(UNIVERSALSPEECH_LIBRARY_DEBUG
		NAMES UniversalSpeechStatic
		PATHS "${CMAKE_CURRENT_LIST_DIR}/../../debug/lib"
		NO_DEFAULT_PATH
	)
	select_library_configurations(UNIVERSALSPEECH)
	set_target_properties(universalspeech::universalspeech PROPERTIES
		IMPORTED_LOCATION_RELEASE "${UNIVERSALSPEECH_LIBRARY_RELEASE}"
		IMPORTED_LOCATION_DEBUG "${UNIVERSALSPEECH_LIBRARY_DEBUG}"
		INTERFACE_INCLUDE_DIRECTORIES "${CMAKE_CURRENT_LIST_DIR}/../../include"
	)
endif()

# Set the universalspeech_FOUND variable
set(universalspeech_FOUND TRUE)
